{
  "comments": {
    "lineComment": ";"
  },
  "autoClosingPairs": [
    {
      "open": "'",
      "close": "'"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "\"",
      "close": "\""
    }
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["[", "]"],
    ["\"", "\""]
  ],
  "indentationRules": {
    "increaseIndentPattern": "\\.*(.+):"
  }
}
